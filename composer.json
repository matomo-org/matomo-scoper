{
    "name": "matomo/matomo-scoper",
    "description": "Prefix composer dependencies in Matomo or a Matomo plugin to avoid conflicts.",
    "type": "project",
    "require-dev": {
        "phpunit/phpunit": "^10.3"
    },
    "license": "GPL-3.0+",
    "autoload": {
        "psr-4": {
            "Matomo\\Scoper\\": "src/",
            "Matomo\\Scoper\\Tests\\": "tests/"
        }
    },
    "bin": ["bin/matomo-scoper"],
    "authors": [
        {
            "name": "The Matomo Team",
            "email": "hello@matomo.org"
        }
    ],
    "require": {
        "symfony/console": "^6.3",
        "guzzlehttp/guzzle": "^7.8",
        "symfony/filesystem": "^6.3"
    },
    "scripts": {
        "test": "phpunit ./tests"
    }
}
